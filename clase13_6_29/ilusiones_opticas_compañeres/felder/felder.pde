void draw () {
  for (int x = 0; x<=12; x++) {
    for (int y = 0; y<=12; y++) {
      if ( y==1 && x>4 && x<8 || x==1 && y>4 && y<8 || x==11 && y>4 && y<8 || y==11 && x>4 && x<8) {
        fill(0);
        //fill(0, 255, 0);
      } else if (y==3 && x>4 && x<8 || x==3 && y>4 && y<8 || x==9 && y>4 && y<8 || y==9 && x>4 && x<8) {
        fill( 0 );
        //fill(0, 200, 0);
      } else if (y==5 && x>=5 && x<=7||x==5 && y>=5 && y<=7||y==7 && x>=5 && x<=7||x==7 && y>=5 && y<=7) {
        fill(0);
        //fill(0, 150, 0);
      } else if (x<=4 && y==0 || y<=4 && x==0 || x>=8 && y==0 || y>=8 && x==0 || x>=8 && y==12 || y>=8 && x==12 ||x<=4 && y==12 || y<=4 && x==12) {
        fill(0);
      } else if (y==2 && x>1 && x<5 ||y==2 && x>7 && x<11||y==10 && x>1 && x<5 ||y==10 && x>7 && x<11) {
        fill(0);
      } else if (x==2 && y>1 && y<5 ||x==2 && y>7 && y<11||x==10 && y>1 && y<5 ||x==10 && y>7 && y<11) {
        fill(0);
      } else if (x==4 && y==4 || x==4 && y==8 || x==8 && y==4 || x==8 && y==8) {
        fill(0);
      } else {
        fill(255);
      }
      rect(x*40, y*40, 40, 40);
      //fill( 0 );
      //text( x+" / " + y, x*40+5, y*40+20 );
    }
  }
}
